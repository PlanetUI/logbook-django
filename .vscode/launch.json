                "runserver"
            ],
        },
        {
            "name": "Django: Makemigrations",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "console": "integratedTerminal",
            "args": [
                "makemigrations"
            ],
        },
        {
            "name": "Django: Migrate",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "console": "integratedTerminal",
            "args": [
                "migrate"
            ],
        },
